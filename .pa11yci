{
  "defaults": {
    "standard": "WCAG2AA",
    "timeout": 30000,
    "chromeLaunchConfig": {
      "args": [
        "--no-sandbox",
        "--disable-dev-shm-usage"
      ]
    },
    "reporters": [
      "cli",
      [
        "json",
        {
          "fileName": "./pa11y/pa11y-ci-results.json"
        }
      ],
      [
        "pa11y-ci-reporter-html",
        {
          "destination": "./pa11y/report-html"
        }
      ]
    ]
  },
  "urls": [
    "http://mobi.meuhedet.co.il"
  ]
}
