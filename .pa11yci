{
  "urls": [
    "http://mobi.meuhedet.co.il/"
  ],
  "defaults": {
    "timeout": 30000,
    "chromeLaunchConfig": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-dev-shm-usage"
      ]
    },
    "reporters": [
      "cli",
      [
        "json",
        {
          "fileName": "pa11y/pa11y-results.json"
        }
      ],
      [
        "pa11y-ci-reporter-html",
        {
          "destination": "pa11y/html"
        }
      ]
    ]
  }
}
